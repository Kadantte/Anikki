  "version": "0.2.0",
  "configurations": [
    {
      "name": "kawanime",
      "request": "launch",
      "type": "dart",
      "env": {
        "KAWANIME_BINDINGS_DIRECTORY": "${workspaceFolder}/lib/bindings"
      }
    },
    {
      "name": "kawanime (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "env": {
        "KAWANIME_BINDINGS_DIRECTORY": "${workspaceFolder}/lib/bindings"
      }
    },
    {
      "name": "kawanime (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "env": {
        "KAWANIME_BINDINGS_DIRECTORY": "${workspaceFolder}/lib/bindings"
      }
    }
  ]
}