{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "anikki",
      "request": "launch",
      "type": "dart",
      "args": [
        "--vmservice-out-file=detective_connect.txt",
      ]
    },
    {
      "name": "anikki with device preview",
      "request": "launch",
      "type": "dart",
      "args": [
        "--vmservice-out-file=detective_connect.txt",
      ],
      "env": {
        "DEVICE_PREVIEW": "on"
      }
    },
    {
      "name": "anikki (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "anikki (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}